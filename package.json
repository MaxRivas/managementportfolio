{
  "name": "management-portfolio",
  "version": "1.0.0",
  "description": "A modern portfolio template for management professionals built with Astro",
  "type": "module",
  "scripts": {
    "dev": "astro dev --config ./astro.config.local.mjs",
    "dev:github": "astro dev",
    "start": "astro dev --config ./astro.config.local.mjs",
    "build": "astro build --config ./astro.config.local.mjs",
    "build:github": "astro build",
    "preview": "astro preview --config ./astro.config.local.mjs",
    "preview:github": "astro preview",
    "test:local": "npm run build && npm run preview",
    "test:github": "npm run build:github && npm run preview:github",
    "run:github": "npm run build:github && npm run preview:github",
    "astro": "astro"
  },
  "keywords": [
    "astro",
    "portfolio",
    "management",
    "tailwindcss",
    "typescript"
  ],
  "author": "Management Portfolio",
  "license": "MIT",
  "dependencies": {
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.1",
    "astro": "^4.16.18",
    "astro-robots-txt": "^1.0.0",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@types/node": "^22.8.4",
    "typescript": "^5.6.3"
  }
}