  <rect x="260" y="150" width="450" height="30" fill="#f8fafc" rx="3"/>
  <text x="485" y="170" text-anchor="middle" font-family="Arial" font-size="14" fill="#0ea5e9" font-weight="bold">
    Project Timeline
  </text>
  
  <!-- Timeline bars -->
  <rect x="280" y="200" width="200" height="20" fill="#10b981" rx="3"/>
  <rect x="290" y="230" width="150" height="20" fill="#f59e0b" rx="3"/>
