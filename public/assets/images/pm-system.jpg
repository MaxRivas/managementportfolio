  <rect x="285" y="320" width="160" height="20" fill="#06b6d4" rx="3"/>
  
  <!-- Status indicators -->
  <circle cx="500" cy="210" r="8" fill="#10b981"/>
  <circle cx="460" cy="240" r="8" fill="#f59e0b"/>
  <circle cx="470" cy="270" r="8" fill="#ef4444"/>
  <circle cx="540" cy="300" r="8" fill="#8b5cf6"/>
  <circle cx="465" cy="330" r="8" fill="#06b6d4"/>
  
  <!-- Resource allocation -->
  <rect x="260" y="360" width="450" height="120" fill="#f8fafc" rx="5"/>
  <text x="485" y="385" text-anchor="middle" font-family="Arial" font-size="14" fill="#0ea5e9" font-weight="bold">
    Resource Allocation
  </text>
  
  <rect x="280" y="400" width="100" height="60" fill="#dbeafe" rx="5"/>
  <rect x="400" y="400" width="100" height="60" fill="#dcfce7" rx="5"/>
  <rect x="520" y="400" width="100" height="60" fill="#fef3c7" rx="5"/>
  
  <text x="400" y="530" text-anchor="middle" font-family="Arial" font-size="24" fill="#0ea5e9" font-weight="bold">
    Project Management System
  </text>
